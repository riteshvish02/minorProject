<%- include('./partials/header'); %>
<%- include('./partials/menu',{formattedDateTime}); %>
<div class="parent w-full h-fit  bg-[#FAE8DC] flex  items-center justify-center">
  <div class="child max-md:w-full md:w-[80%] h-fit  bg-white shadow-md	box-shadow: 0 4px 6px -1px rgb(0 0 0 / 0.1), 0 2px 4px -2px rgb(0 0 0 / 0.1);">
   <div class="hero w-full  h-fit ">
     <div class="flex flex-col px-2 md:flex-row">
       <div class="flex flex-col items-center left-content max-md:w-full    max-md:px-2 md:w-[75%] pl-[3vh] pt-[5vh] h-full  ">
        <div class="head xl:w-[55vw] max-lg:p-3 xl:h-[11vh] lg:p-3 bg-[#025A6C] rounded-md flex items-center justify-center">
          <h2 class="text-white uppercase xl:text-[5vh] xl:text-[2.5vh] font-bold md:text-[2.5vw] lg:text-[4vw] text-center">Editorial board</h2>
      </div>
          <%if(editorials){%>
        
             <%editorials.forEach(function(editorial) {%>
                <%if(editorial.type == 'Editor in chief'){%>
                    <div class="editor-chief-associate shadow-md  border-[1.5px] cards md:w-[35vw] max-lg:w-full mt-7 py-5  flex flex-col items-center px-[1.2vh] rounded-md max-md:min-h-[40vh]  min-h-[25vw] bg-[#F0F0F0] ">
                        <h1 class="text text-[black] xl:text-[4vh] md:text-[3vw] text-[3.5vw]  uppercase font-bold">Editor in Chief</h1>
                        <div class="img xl:h-[15vh] xl:w-[15vh] md:w-[9vw] md:h-[9vw] h-[25vw] w-[25vw] bg-[#849a9f] rounded mt-3 bg-[#849a9f] rounded p-1">
                            <img class="w-full rounded h-full object-center object-cover" src="/images/uploads/<%=editorial.image.url%>" alt="">
                        </div>
                        <h2 class="text-[#212529] font-medium mt-3 md:text-[2vw] text-[3vw] xl:text-[3vh] capitalize"><%=editorial.name%></h2>
                        <h3 class="xl:text-[2.5vh] md:text-[1.3vw] text-[2.9vw] text-[#FF6600] text-center mt-3"><%=editorial.university%></h3>
                        <h4 class="xl:text-[2.5vh] md:text-[1.3vw] text-[2.9vw] font-normal text-center  mr-2 mt-3">
                            <%-editorial.description%>
                        </h4>
                  </div>
                <%}%>
                <%})%>
               
           <%}else{%>
            <h2 class="text-[5vh] capitalize font-medium mt-3">No Editorial</h2>
            <%}%>
           
         
            <div class="w-full mt-[5vh]">
              <h2 class="xl:text-[3.7vh] text-[4.2vw] md:text-[2.6vw] font-medium">Editorial board members</h2>
            </div>
             <%editorials && editorials.forEach((editorial,index)=>{%>
              
             <%if(editorial.type == "Editorial board member"){%>
                 <div class="cards members flex-col md:flex-row w-[90%] min-h-[15vw] mt-3  pt-5 flex bg-[#E2DAD6] rounded-md shadow-sm border-[1.2px]">
                     <div class="left md:w-[25%]  flex justify-center h-full">
                     <div class="img xl:h-[15vh] xl:w-[15vh] md:w-[9vw] md:h-[9vw] h-[25vw] w-[25vw] p-1 bg-white">
                         <img class="object-center rounded  object-cover w-full h-full" src="/images/uploads/<%=editorial.image.url%>" alt="">
                     </div>
                     </div>
                     <div class="right md:w-[75%] max-lg:px-3 max-lg:py-2 h-full">
                         <h1 class="text-black md:text-[2vw] text-[3vw] xl:text-[3.5vh] font-bold"><%=editorial.name%></h1>
                         <h3 class="xl:text-[2.5vh] md:text-[1.3vw] text-[2.9vw] text-red-500 max-md:text-[2vh]  mt-3"><%=editorial.university%></h3>
                         <h4 class="xl:text-[2.5vh] md:text-[1.3vw] text-[2.9vw] text-zinc-800 max-md:text-[1.8vh] font-normal mt-3  mr-2">
                            <%-editorial.description%>
                         </h4>
                     </div>
               </div>
             <%}%>
               
           
             
           <%})%>
           
          <div class="w-full mt-[5vh]">
            <h2 class="xl:text-[3.7vh] text-[4.2vw] md:text-[2.6vw] font-medium">Advisory board members</h2>
          </div>
           <%editorials && editorials.forEach((editorial,index)=>{%>
            
            <%if(editorial.type == "Advisory board member"){%>
                <div class="cards members flex-col md:flex-row w-[90%] min-h-[15vw] mt-3  pt-5 flex bg-[#E2DAD6] rounded-md shadow-sm border-[1.2px]">
                    <div class="left md:w-[25%]  flex justify-center h-full">
                    <div class="img xl:h-[15vh] xl:w-[15vh] md:w-[9vw] md:h-[9vw] h-[25vw] w-[25vw] p-1 bg-white">
                        <img class="object-center rounded  object-cover w-full h-full" src="/images/uploads/<%=editorial.image.url%>" alt="">
                    </div>
                    </div>
                    <div class="right md:w-[75%] max-lg:px-3 max-lg:py-2 h-full">
                        <h1 class="text-black md:text-[2vw] text-[3vw] xl:text-[3.5vh] font-bold"><%=editorial.name%></h1>
                        <h3 class="xl:text-[2.5vh] md:text-[1.3vw] text-[2.9vw] text-red-500 max-md:text-[2vh]  mt-3"><%=editorial.university%></h3>
                        <h4 class="xl:text-[2.5vh] md:text-[1.3vw] text-[2.9vw] text-zinc-800 max-md:text-[1.8vh] font-normal mt-3  mr-2">
                           <%-editorial.description%>
                        </h4>
                    </div>
              </div>
            <%}%>
         
           
         <%})%>
        
      </div>
    <%- include('./partials/sidebar',{news});%>
     </div>
     <div class="flex flex-wrap p-3"> 
  
  
      <div class="w-full transition-transform duration-0.2 ease hover:scale-[1.03] cursor-pointer sm:w-1/2 md:w-1/3 flex flex-col p-3">
        <div class="bg-white rounded-lg shadow-lg overflow-hidden flex-1 flex flex-col">
          <div class="bg-cover xl:h-48 max-xl:py-5 flex items-center justify-center bg-[#058ECB]">
            <h6 class="xl:text-[2.5vw] md:text-[2vw] text-[6vw] capitalize text-center text-white font-bold">Call for papers</h6>
          </div>
          <div class="p-4 flex-1 flex flex-col">
            <h3 class="mb-4 xl:text-2xl md:text-[1.7vw] text-[5vw] text-center font-bold">24x7 working</h3>

            <a href="/manuscript-submission" class="bg-[#058ECB] p-2 xl:text-2xl md:text-[1.7vw] text-[3vw] hover:text-red uppercase no-underline tracking-wide mx-auto text-white font-bold uppercase">submit today</a>
          </div>
        </div>  
      </div>
      
      <div class="w-full  transition-transform duration-0.2 ease hover:scale-[1.03] cursor-pointer sm:w-1/2 md:w-1/3 flex flex-col p-3">
        <div class="bg-white rounded-lg shadow-lg overflow-hidden flex-1 flex flex-col">
          <div class="bg-cover xl:h-48 max-xl:py-5 flex items-center justify-center bg-[#82B26C]">
            <h6 class="xl:text-[2.5vw] md:text-[2vw] text-[6vw] capitalize text-center text-white font-bold">Notification of acceptance</h6>
          </div>
          <div class="p-4 flex-1 flex flex-col">
            <h3 class="mb-4 xl:text-2xl md:text-[1.7vw] text-[5vw] text-center font-bold">Within 24 hours</h3>

            <a href="/manuscript-submission" class="bg-[#82B26C] p-2 xl:text-2xl md:text-[1.7vw] text-[3vw] hover:text-red uppercase no-underline tracking-wide mx-auto text-white font-bold uppercase">submit today</a>
          </div>
        </div>  
      </div>
      <div class="w-full  transition-transform duration-0.2 ease hover:scale-[1.03] cursor-pointer sm:w-1/2 md:w-1/3 flex flex-col p-3">
        <div class="bg-white rounded-lg shadow-lg overflow-hidden flex-1 flex flex-col">
          <div class="bg-cover xl:h-48 max-xl:py-5 flex items-center justify-center bg-[#058ECB]">
            <h6 class="xl:text-[2.5vw] md:text-[2vw] text-[6vw] capitalize text-center text-white font-bold">E-certificates</h6>
          </div>
          <div class="p-4 flex-1 flex flex-col">
            <h3 class="mb-4 xl:text-2xl md:text-[1.7vw] text-[5vw] text-center font-bold">Within 04 hours</h3>

            <a href="/manuscript-submission" class="bg-[#058ECB] p-2 xl:text-2xl md:text-[1.7vw] text-[3vw] hover:text-red uppercase no-underline tracking-wide mx-auto text-white font-bold uppercase">submit today</a>
          </div>
        </div>  
      </div>
      
     
        
    </div>
     <div class="relative max-md:hidden w-[95%] m-auto md:h-[10vw] max-md:h-[15vh] my-3 items-center flex border-[1.3px] border-black">
       <%- include('./partials/scroller',{indexings}); %>
    </div>
    <div class="relative md:hidden w-[85%] m-auto md:h-[20vh] max-md:h-[15vh] my-3 items-center flex border-[1.3px] border-black">
     <%- include('./partials/resscroller',{indexings}); %>
   </div>
   </div>
  
     <%- include('./partials/footerfrontend',{visit}); %>
  </div>
 </div>
<%- include('./partials/footer'); %>
